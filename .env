VITE_APP_TITLE=datacat editor
VITE_API_URL=http://localhost:8080/graphql

# Path to catalog information markdown file
# In development: loads from public folder
# In Docker: should be set to volume path like /volumes/catalog-info.md
VITE_CATALOG_INFO_PATH=/catalog-info.md

# Direkte MinIO Konfiguration (ohne Presigned URLs)
VITE_MINIO_ENDPOINT=localhost
VITE_MINIO_PORT=9000
VITE_MINIO_USE_SSL=false
VITE_MINIO_BUCKET_NAME=ids-files
VITE_MINIO_ACCESS_KEY=admin
VITE_MINIO_SECRET_KEY=dcstorage